@model IEnumerable<SupplierRanking.Models.Fornecedor>

@{
    ViewBag.Title = "listaFornecedor";
}

<h2>listaFornecedor</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cnpj)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome_empresa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cnpj)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nome_empresa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        
        <td>        
            @Html.ActionLink("Delete", "ExcluirContaFornecedor", new {  item.Cnpj })
        </td>
    </tr>
}

</table>
