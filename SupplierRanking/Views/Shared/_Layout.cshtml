<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>@ViewBag.Title - Supplier Ranking</title>

        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Quicksand:300,400,500,700">
        <link rel="stylesheet" href="~/Content/config/css/open-iconic-bootstrap.min.css">
        <link rel="stylesheet" href="~/Content/config/css/animate.css">

        <link rel="stylesheet" href="~/Content/config/css/owl.carousel.min.css">
        <link rel="stylesheet" href="~/Content/config/css/owl.theme.default.min.css">
        <link rel="stylesheet" href="~/Content/config/css/magnific-popup.css">
        <link rel="stylesheet" href="~/Content/config/css/aos.css">
        <link rel="stylesheet" href="~/Content/config/css/ionicons.min.css">
        <link rel="stylesheet" href="~/Content/config/css/bootstrap/bootstrap-datepicker.css">
        <link rel="stylesheet" href="~/Content/config/css/jquery.timepicker.css">

        <link rel="stylesheet" href="~/Content/config/css/flaticon.css">
        <link rel="stylesheet" href="~/Content/config/css/icomoon.css">
        <link rel="stylesheet" href="~/Content/config/css/style.css">

        <link rel="stylesheet" href="~/Content/css/style.css">

        <!-- Logo da Aba -->
         <link rel="icon" href="~/Content/images/logo.png" type="image/x-icon" />
    </head>

    <body>
        <!-- INÍCIO - MENU -->
        <nav id="ftco-navbar"
             class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light"
             data-aos="fade-down" data-aos-delay="500">
            <div class="container">
                <a class="navbar-brand" href="/Home/Index">
                   <img style="width:60px;" src="~/Content/images/logo.png" />
                     Supplier Ranking
                </a>

                <!-- INÍCIO - Menu Hamburguer -->
                <button class="navbar-toggler"
                        type="button"
                        data-toggle="collapse"
                        data-target="#ftco-nav"
                        aria-controls="ftco-nav"
                        aria-expanded="false"
                        aria-label="Toggle navigation">
                        <span class="oi oi-menu"></span> Menu
                </button>
                <!-- FIM - Menu Hamburguer -->

                <div id="ftco-nav"
                     class="collapse navbar-collapse">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <a class="nav-link" href="/Home/Index">
                                Home
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/Home/SobreNos">
                                Sobre Nós
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/Home/Planos">
                                Planos
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/Home/Contato">
                                Contato
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/Login/Login">
                                Acessar
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <!-- FIM - MENU -->

        <!-- INÍCIO - CONTEÚDO -->
        <div>
            @RenderBody()
        </div>
        <!-- FIM - CONTEÚDO -->

        <!-- INÍCIO - RODAPÉ -->
        <footer class="ftco-footer ftco-section">
            <div class="container">
                <div class="row mb-2">
                    <div class="col-md-5">
                        <div class="ftco-footer-widget mb-4">
                            <h2 class="ftco-heading-2">Informações de Contato:</h2>
                            <ul class="list-unstyled">
                                <li>
                                    <a class="py-2 d-block"
                                        href="">
                                        Av. da Saudade, 125 - Ponte Preta, Campinas - SP
                                    </a>
                                </li>
                                <li>
                                    <a class="py-2 d-block"
                                        href="">
                                        (19) 3231-2540
                                    </a>
                                </li>
                                <li>
                                    <a class="py-2 d-block"
                                        href="mailto:suportesupplierranking@hotmail.com">
                                        suportesupplierranking@hotmail.com
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-1">
                        <div class="ftco-footer-widget mb-4">
                            <ul class="ftco-footer-social list-unstyled float-md-right float-lft">
                                <li class="ftco-animate"><a href=""><span class="icon-twitter"></span></a></li>
                                <li class="ftco-animate"><a href=""><span class="icon-facebook"></span></a></li>
                                <li class="ftco-animate"><a href=""><span class="icon-instagram"></span></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 text-center">
                    <p>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        Supplier Ranking &copy;
                        <script>document.write(new Date().getFullYear());</script> Todos os direitos reservados | Site desenvolvido pelos criados do Supplier Ranking <i class="icon-compass" aria-hidden="true">
                        </i> por: <a href="Home/SobreNos" target="_blank">Luiz Gustavo | Laressa Assis | Marcelo Lemos | Valmir Junior</a>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    </p>
                </div>
            </div>            
        </footer>
        <!-- FIM - RODAPÉ -->

        <!-- loader -->
        <div id="ftco-loader" class="show fullscreen">
            <svg class="circular" width="48px" height="48px">
                <circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" />
                <circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#0C496BE6" />
            </svg>
        </div>

        <script src="~/Content/config/js/jquery.min.js"></script>
        <script src="~/Content/config/js/jquery-migrate-3.0.1.min.js"></script>
        <script src="~/Content/config/js/popper.min.js"></script>
        <script src="~/Content/config/js/bootstrap.min.js"></script>
        <script src="~/Content/config/js/jquery.easing.1.3.js"></script>
        <script src="~/Content/config/js/jquery.waypoints.min.js"></script>
        <script src="~/Content/config/js/jquery.stellar.min.js"></script>
        <script src="~/Content/config/js/owl.carousel.min.js"></script>
        <script src="~/Content/config/js/jquery.magnific-popup.min.js"></script>
        <script src="~/Content/config/js/aos.js"></script>
        <script src="~/Content/config/js/jquery.animateNumber.min.js"></script>
        <script src="~/Content/config/js/bootstrap-datepicker.js"></script>
        <script src="~/Content/config/js/jquery.timepicker.min.js"></script>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
        <script src="~/Content/config/js/google-map.js"></script>
        <script src="~/Content/js/main.js"></script>
    </body>
</html>