<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>@ViewBag.Title - Supplier Ranking</title>

        <!-- INICIO - Nossos CSS -->
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Quicksand:300,400,500,700">
        <link rel="stylesheet" href="~/Content/config/css/style.css"/>
        <link rel="stylesheet" href="~/Content/css/style_login.css" />
        <link rel="stylesheet" href="~/Content/css/login.css" />
        <link href="~/Content/css/nova-senha.css" rel="stylesheet" />
        
        <!-- FIM - Nossos CSS -->

        <!-- Logo da Aba -->
        <link rel="icon" href="~/Content/images/logo.png" type="image/x-icon" />
    </head>
    <body>
        <!-- INICIO - Conteudo -->
        <div class="container-login">
            <div class="wrap-login justify-content-center">
               @RenderBody()
            </div>
        </div>
        <!-- FIM - Conteudo -->

        <!-- INÍCIO - Script's de configuracao -->
        <script src="~/Content/vendor/jquery/jquery-3.2.1.min.js"></script>
        <script src="~/Scripts/jquery.mask.js"></script>

        <script src="~/Content/vendor/bootstrap/js/popper.js"></script>
        <script src="~/Content/vendor/bootstrap/js/popper.min.js"></script>
        <script src="~/Content/vendor/bootstrap/js/bootstrap.js"></script>
        <script src="~/Content/vendor/bootstrap/js/bootstrap.min.js"></script>
        <script src="~/Content/vendor/bootstrap/js/tooltip.js"></script>

        <script src="~/Content/vendor/select2/select2.js"></script>
        <script src="~/Content/vendor/select2/select2.min.js"></script>

        <script src="~/Content/vendor/tilt/tilt.jquery.min.js"></script>
        <script>
            $('.js-tilt').tilt({
                scale: 1.1
            })
        </script>
        <!-- FIM - Script's de configuracao -->

        <!-- INÍCIO - Nossos JS -->
        <script src="~/Content/js/login.js"></script>
        <script src="~/Content/js/esqueceu-senha.js"></script>
        <script src="~/Content/js/esqueceu-senha-comprador.js"></script>
        <script src="~/Content/js/esqueceu-senha-fornecedor.js"></script>
        
    </body>
</html>
