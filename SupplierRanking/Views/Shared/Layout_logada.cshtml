            <nav id="sidebar">
                <div class="sidebar-header" style="height: 104px;">
                    <h3>Supplier Ranking</h3>
                </div>

                <!-- Perfil do login -->
                <ul class="list-unstyled components">
                    <a style="display:flex; justify-content:center;">Bem-vindo!</a>
                    @if (Session["UserFornecedor"] != null)
                    {
                        <a href="/HomeLogada/Perfil/?Cnpj=@Session["UserFornecedor"].ToString()">
                            <img src="/Content/images/iconecor.png" id="perfil" style="width: 100px; height: 100px; border-radius: 50px; border: 1px solid white; margin: 10px auto auto 25px;" />
                        </a>
                        <a style="font-family:inherit; font-size: 20px; display:flex; justify-content:center;">@Session["NomeUsuario"].ToString()</a>
                    }
                    else if (Session["UserPessoaFisica"] != null || Session["UserPessoaJuridica"] != null)
                    {
                        <a style="font-family:inherit; font-size: 20px; display:flex; justify-content:center;">@Session["NomeUsuario"].ToString()</a>
                        <img src="~/Content/images/user.png" id="perfil" style="width: 100px; height: 100px; border-radius: 50px; border: 1px solid white; margin: 10px auto auto 25px;" />
                    }
                    <br />
                    <ul class="list-unstyled components"></ul>

                    <li>
                        @if (Session["UserPessoaFisica"] != null)
                        {
                            <a style="text-decoration: none;font-family: inherit;"
                            href="/Comprador/UpdatePessoaFisica/?Codigo=@Session["CodigoUsuario"].ToString()" aria-expanded="false">Editar Perfil</a>
                        }
                        else if (Session["UserPessoaJuridica"] != null)
                        {
                            <a style="text-decoration: none;font-family: inherit;"
                            href="/Comprador/UpdatePessoaJuridica/?Codigo=@Session["CodigoUsuario"].ToString()" aria-expanded="false">Editar Perfil</a>
                        }
                        else if (Session["UserFornecedor"] != null)
                        {
                            <a style="text-decoration: none;font-family: inherit;" 
                            href="/Fornecedor/UpdateFornecedor/?Cnpj=@Session["UserFornecedor"].ToString()" aria-expanded="false">Editar Perfil</a>
                        }
                    </li>

                    <li>
                        <a style="text-decoration: none; font-family: inherit;"
                        href="/Comprador/UpdateSenha/" aria-expanded="false">Alterar Senha</a>
                    </li>

                    <li>
                        <a style="text-decoration: none; font-family: inherit;"
                        href="/Home/Sair/" aria-expanded="false">Encerrar Sessão</a>
                    </li>
                </ul>


                    <ul class="list-unstyled components">
                        <li class="active">
                            <a href="/HomeLogada/RankingGeral" aria-expanded="false">Ranking Geral</a>
                        </li>
                        @*<li>
                    <a href="/HomeLogada/Pesquisa"  aria-expanded="false">Pesquisar</a>
                </li>*@

                        <li>
                            <a href="/HomeLogada/RankingPremium" aria-expanded="false">Ranking Premium</a>
                        </li>

                        <li>
                            <a href="/HomeLogada/Suporte" aria-expanded="false">Suporte</a>
                        </li>

                        <li>
                            @*<a href="#homeSubmenu" data-toggle="collapse" aria-expanded="false" >Sair</a>*@


                            @*@Html.ActionLink("Editar informações", "UpdatePessoaFisica", "Comprador", new { area = "" }, new { @class = "" })*@

                        </li>
                    </ul>



            </nav>
            <!---- Modal ---->
            <div class="modal fade" id="myModal" role="dialog">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content">
                        <div class="modal-header" style="
                            background-color: #0C496BE6;
                            color: white;
                            border-bottom: 3px solid #bbad00; ">

                            <h4 style="margin-left: 380px;" class="modal-title">Filtro </h4>
                            <button type="button" class="close" data-dismiss="modal" style="color:white;">×</button>

                        </div>

                        <form method="post" action="../HomeLogada/Pesquisa">
                            <div class="modal-body">
                                <p>Filtrar por:</p>


                                <label style="margin-left: 4px;" class="container">
                                    Entrega

                                    <input value="media_entrega" id="entrega" type="radio" checked="checked" name="pesquisa" style="visibility:hidden">
                                    <span style="margin-left: -15px;" class="checkmark"></span>
                                </label>
                                <label style="margin-left: 4px;" class="container">
                                    Qualidade
                                    <input value="media_qualidade" id="qualidade" type="radio" name="pesquisa" style="visibility:hidden">
                                    <span style="margin-left: -15px;" class="checkmark"></span>
                                </label>
                                <label style="margin-left: 4px;" class="container">
                                    Preço
                                    <input value="media_preco" id="preco" type="radio" name="pesquisa" style="visibility:hidden">
                                    <span style="margin-left: -15px;" class="checkmark"></span>
                                </label>
                                <label style="margin-left: 4px;" class="container">
                                    Satisfação
                                    <input value="media_satisfacao" id="satisfacao" type="radio" name="pesquisa" style="visibility:hidden">
                                    <span style="margin-left: -15px;" class="checkmark"></span>
                                </label>
                                <label style="margin-left: 4px;" class="container">
                                    Atendimento
                                    <input value="media_atendimento" id="atendimento" type="radio" name="pesquisa" style="visibility:hidden">
                                    <span style="margin-left: -15px;" class="checkmark"></span>
                                </label>

                            </div>
                            <div class="modal-footer">
                                <button type="submit" class="btn btn-default">Concluir</button>
                                <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>

                            </div>
                        </form>





                    </div>
                </div>
            </div>
                        </div>

        <div class="conteudo-logada row pl-5 pr-5 col-11" style="top: 105px;">
